# Copy to .env and fill with your values from Meta (WhatsApp Cloud API dashboard)
# Never commit real secrets

# Long-lived access token from WhatsApp Cloud API
WHATSAPP_ACCESS_TOKEN=

# Phone Number ID shown in the WhatsApp Manager dashboard (not the phone number itself)
# e.g. 845243325338071 (replace with your own)
WHATSAPP_PHONE_NUMBER_ID=

# Webhook verify token you set in the Meta App dashboard
WEBHOOK_VERIFY_TOKEN=

# Graph API version; keep in sync with your app settings
WHATSAPP_API_VERSION=v22.0

# Local port for the server
PORT=
